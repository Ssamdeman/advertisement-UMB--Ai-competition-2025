{
  "name": "AI Ad Director",
  "dockerFile": "Dockerfile",
  "forwardPorts": [3000, 5000],
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "ms-python.python",
        "bradlc.vscode-tailwindcss"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      }
    }
  },
  "postCreateCommand": "npm install && cd frontend && npm install && cd .. && pip install -r requirements.txt",
  "remoteUser": "node"
}